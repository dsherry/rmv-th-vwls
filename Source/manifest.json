{
    "manifest_version": 2,
    "name": "RMV TH VWLS",
    "short_name": "RMV TH VWLS",
    "version": "1.0",

    "author": "Dylan Sherry",
    "description": "Rmvs ll th vwls frm yr txt. 'Y' dsn't cnt.",
    "icons":
    {
        "16": "on.png",
        "128": "on_large.png"
    },

    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*",
        "storage"
    ],

    "browser_action": {
        "default_icon": "off.png",
        "default_title": "RMV TH VWLS"
    },

    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js": ["jquery-2.2.0.min.js", "content_script.js"],
            "run_at": "document_end"
        }
    ],
    "background": {
        "scripts": ["background.js"]
    }
}


{
	"manifest_version": 2,
	"name": "RMV TH VWLS",
	"version": "1.0",
	"description": "Rmvs ll th vwls frm yr txt. 'Y' dsn't cnt.",
	"content_scripts":
	[
		{
			"matches": ["*://*/*"],
		        "js": ["jquery-2.2.0.min.js", "content_script.js"],
			"run_at": "document_end"
		}
	],
        "permissions": [
            "tabs",
            "http://*/*",
            "https://*/*",
            "storage"
        ],
        "icons":
        {
                "16": "on.png",
                "128": "on_large.png"
        },
        "background": {
                "scripts": ["background.js"]
        },
        "browser_action": {
                "default_icon": "off.png",
                "default_title": "icon"
        }
}

